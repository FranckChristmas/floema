extends ../base.pug 

block variables 
  - var template  = 'About'

block content
  .about 
    .about__wrapper 
      section.about__gallery 
        .about__gallery__wrapper 
          each media in about.data.gallery 
            figure.about__gallery__media
              img.about__gallery__media__image(alt=media.image.alt src=media.image.url)

  block modal 
    .modal 
      .modal__wrapper